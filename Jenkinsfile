pipeline {
  agent any
  stages {
    stage('Hello') {
      steps {
        sh 'echo "Hello world!"'
      }
    }

    stage('Test pipline') {
      steps {
        sh 'echo "Test pipline in jenkins"'
      }
    }

    stage('please workin') {
      steps {
        sh 'echo "please workin"'
      }
    }

  }
}